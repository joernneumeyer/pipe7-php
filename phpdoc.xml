<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
>
    <title>Pipe7</title>
    <paths>
        <output>docs/</output>
        <cache>docs/.cache</cache>
    </paths>
    <version number="3.0">
        <api format="php">
            <ignore>
                <path>vendor/**/*</path>
                <path>benchmark/**/*</path>
                <path>tests/**/*</path>
            </ignore>
            <visibility>public</visibility>
        </api>
    </version>
</phpdocumentor>
